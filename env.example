# Environment Variables Example
# Copy this file to .env.local for local development

# AWS Configuration (for deployment)
REACT_APP_AWS_REGION=us-east-1
REACT_APP_S3_BUCKET=your-s3-bucket-name

# Lambda Function Configuration
REACT_APP_DEFAULT_LAMBDA_URL=https://your-lambda-function-url.lambda-url.region.on.aws/

# Application Configuration
REACT_APP_DEFAULT_TABLE_NAME=users
REACT_APP_APP_NAME="Provider Utility"
REACT_APP_APP_DESCRIPTION="React frontend for AWS Lambda MariaDB integration"

# Development Configuration
GENERATE_SOURCEMAP=false
REACT_APP_ENVIRONMENT=development

